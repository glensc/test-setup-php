{
  "problemMatcher": [
    {
      "owner": "phpunit",
      "pattern": [
        {
          "regexp": "^\\d+\\)\\s.*$"
        },
        {
          "regexp": "^(.*Failed\\sasserting\\sthat.*|(^[@ +-]+|^[}\"\\]]).*)$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
