{
  "problemMatcher": [
    {
      "owner": "phpunit",
      "pattern": [
        {
          "regexp": "^\\d+\\)\\s.*$"
        },
        {
          "regexp": "(.*Failed\\sasserting\\sthat.*(?:\\r\\n|\\n(?!^\\s*$).*)*)$|^\\s*$|^(.*):(\\d+)",
          "message": 1,
          "file": 1,
          "line": 2
        }
      ]
    }
  ]
}
