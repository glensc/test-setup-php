{
    "name": "shivammathur/countrycity",
    "description": "API to get all countries and all cities in a country.",
    "keywords": [
        "cities",
        "countries",
        "php",
        "rest",
        "dropdown",
        "psr7",
        "geodata"
    ],
    "homepage": "http://github.com/shivammathur",
    "license": "MIT",
    "authors": [
        {
            "name": "Shivam Mathur",
            "email": "contact@shivammathur.com",
            "homepage": "https://shivammathur.com",
            "role": "author"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "halaxa/json-machine": "*",
        "slim/http-cache": "^0.4.0",
        "slim/php-view": "^3.0",
        "slim/slim": "^3.12",
        "symfony/event-dispatcher": "*",
        "symfony/http-kernel": "*",
        "symfony/process": "*"
    },
    "require-dev": {
        "nikic/php-parser": "*",
        "phpunit/php-code-coverage": "*",
        "phpunit/phpunit": "9.3.5",
        "sebastian/global-state": "*",
        "sebastian/type": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "ext-pcov": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CountryCity\\API\\": "api/v1/",
            "CountryCity\\Templates\\": "templates/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CountryCity\\Tests\\API\\": "tests/api/v1"
        }
    },
    "minimum-stability": "dev",
    "support": {
        "email": "contact@shivammathur.com",
        "issues": "https://github.com/shivammathur/countrycity/issues"
    }
}
